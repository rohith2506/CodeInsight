<!DOCTYPE html>
<html>
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body style="min-width:1300px;position:relative;background:  url(https://cdn.mxpnl.com/cache/edaeb8e0378efb274b717d49aa5db114/images/static/purple_grid.png) #323355; /*background: url(/assets/back1.jpg);background-repeat:no-repeat;background-size:100% */">
	<%= render 'layouts/header' %>
	<div class="container">
		<% flash.each do |key,value| %>
			<div class="alert alert-<%= key %>"><%=value %> </div>
		<% end %>
		<%= yield %>
		<%= render 'shared/sidePanel' %>
		<%= render 'layouts/footer' %>
		<%= debug(params) if Rails.env.development? %>
	</div>
</body>
</html>
